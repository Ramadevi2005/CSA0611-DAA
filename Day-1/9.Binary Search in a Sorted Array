def binary_search(arr, key):
    arr.sort() 
    left, right = 0, len(arr) - 1
    while left <= right:
        mid = (left + right) // 2
        if arr[mid] == key:
            return f"Element {key} is found at position {mid}"
        elif arr[mid] < key:
            left = mid + 1
        else:
            right = mid - 1
    return f"Element {key} is not found"
print(binary_search([3, 4, 6, -9, 10, 8, 9, 30], 10))  
print(binary_search([3, 4, 6, -9, 10, 8, 9, 30], 100)) 
# Output: Element 10 is found at position 5
# Output: Element 100 is not found
