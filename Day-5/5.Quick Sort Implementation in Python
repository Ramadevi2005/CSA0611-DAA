def quick_sort(arr):
    if len(arr) <= 1:
        return arr
    pivot = arr[0]
    left = [x for x in arr[1:] if x <= pivot]
    right = [x for x in arr[1:] if x > pivot]
    sorted_arr = quick_sort(left) + [pivot] + quick_sort(right)
    print(sorted_arr)
    return sorted_arr
# Test Cases
print("Sorted Output:", quick_sort([10, 16, 8, 12, 15, 6, 3, 9, 5]))
print("Sorted Output:", quick_sort([12, 4, 78, 23, 45, 67, 89, 1]))
print("Sorted Output:", quick_sort([38, 27, 43, 3, 9, 82, 10]))
