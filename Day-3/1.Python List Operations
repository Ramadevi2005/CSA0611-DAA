def process_list(lst):
    return sorted(lst)
print(process_list([]))               
print(process_list([1]))              
print(process_list([7, 7, 7, 7]))    
print(process_list([-5, -1, -3, -2, -4]))  
# Expected Output: []
# Expected Output: [1]
# Expected Output: [7, 7, 7, 7]
# Expected Output: [-5, -4, -3, -2, -1]
